   "source": [
    "marks = [45, 78, 90, 33, 60]\n",
    "\n",
    "pass_count = 0\n",
    "fail_count = 0\n",
    "\n",
    "for mark in marks:\n",
    "    if mark >= 50:\n",
    "        pass_count += 1\n",
    "    else:\n",
    "        fail_count += 1\n",
    "\n",
    "print(\"Total Pass Students:\", pass_count)\n",
    "print(\"Total Fail Students:\", fail_count)"
   ]
