    "# Function to filter premium crops\n",
    "def premium_crops(price_list):\n",
    "\n",
    "    premium = []\n",
    "\n",
    "    # Filter prices above 2000\n",
    "    for price in price_list:\n",
    "        if price > 2000:\n",
    "            premium.append(price)\n",
    "\n",
    "    print(\"Premium Crops:\", premium)\n",
    "\n",
    "\n",
    "premium_crops([1500, 2500, 1800, 3200])"
   ]
