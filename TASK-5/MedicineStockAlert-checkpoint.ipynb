    "# Function to check medicine stock\n",
    "def medicine_stock(stock):\n",
    "\n",
    "    if stock < 10:\n",
    "        status = \"Low Stock Alert\"\n",
    "    else:\n",
    "        status = \"Stock Sufficient\"\n",
    "\n",
    "    print(\"Medicine Stock:\", stock)\n",
    "    print(\"Status:\", status)\n",
    "\n",
    "\n",
    "medicine_stock(6)"
   ]
